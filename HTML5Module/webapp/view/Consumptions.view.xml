<mvc:View  controllerName="ns.HTML5Module.controller.Consumptions" 
    xmlns="sap.suite.ui.microchart"
	xmlns:m="sap.m"
	xmlns:layout="sap.ui.layout"
	xmlns:mvc="sap.ui.core.mvc" displayBlock="true">
<m:Page id="pageId" title="Consumptions" floatingFooter="true" backgroundDesign="Solid" showNavButton="true" navButtonPress=".onNavBack">

    <m:headerContent>

        <m:Button icon="sap-icon://action" tooltip="Share" id="button0"/>

        

    </m:headerContent>

    <m:content>
    
    	<m:FlexBox  height="15rem" alignItems="Center" class="sapUiSmallMargin">
			<m:items>
				<InteractiveLineChart id="1SProvince" selectionChanged="selectionChanged" press="press" precedingPoint="15"
									  succeedingPoint="89">
					<points>
                        <InteractiveLineChartPoint id="m01" value="69.9" label="Jan"/>
                        <InteractiveLineChartPoint id="m02" value="69.9" label="Feb"/>
                        <InteractiveLineChartPoint id="m03" value="69.9" label="March"/>
                        <InteractiveLineChartPoint id="m04" value="69.9" label="April"/>
						<InteractiveLineChartPoint id="m05" value="33.1" label="May"/>
						<InteractiveLineChartPoint id="m06" value="12" label="June"/>
					</points>
				</InteractiveLineChart>
                <InteractiveLineChart id="2SProvince" selectionChanged="selectionChanged" press="press" precedingPoint="15"
				    visible="false" succeedingPoint="89">
					<points>
						<InteractiveLineChartPoint id="m07" value="51.4" label="July"/>
						<InteractiveLineChartPoint id="m08" value="52" label="Aug"/>
						<InteractiveLineChartPoint id="m09" value="69.9" label="Sep"/>
						<InteractiveLineChartPoint id="m10" value="0.9" label="Oct"/>
                        <InteractiveLineChartPoint id="m11" value="69.9" label="Nov"/>
                        <InteractiveLineChartPoint id="m12" value="69.9" label="Dec"/>
					</points>
				</InteractiveLineChart>
			</m:items>
		</m:FlexBox>
       
    </m:content>
    <m:footer>
        <m:OverflowToolbar id="toolbar1">

            <m:ToolbarSpacer/>

            <m:Button type="Accept" text="Accept"/>

        </m:OverflowToolbar>
    </m:footer>

    </m:Page>

</mvc:View>