<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:layout="sap.ui.layout" xmlns:semantic="sap.f.semantic" height="100%" controllerName="ns.HTML5Module.controller.App" displayBlock="true">

<semantic:SemanticPage id="mySemanticPage" headerPinnable="true" toggleHeaderOnTitleClick="true" preserveHeaderStateOnScroll="false" titleAreaShrinkRatio="1:1.6:1.6" showFooter="true">

    <!-- Title Heading-->
    <semantic:titleHeading>
        <Title text="{Tech>Lastname}, {Tech>Name}" id="title0"/>
    </semantic:titleHeading>

	<!-- Title Snapped On Mobile Content-->
	<semantic:titleSnappedOnMobile>
			<Title text="Header Title On Phone"/>
	</semantic:titleSnappedOnMobile>

	<!-- Title Content-->

		<!-- Header Content -->
		<semantic:headerContent>
			<layout:HorizontalLayout allowWrapping="true">
				<layout:VerticalLayout class="sapUiMediumMarginEnd">
					<ObjectAttribute xmlns:cd="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" title="Id" text="{Tech>Idtech}" cd:w5g.dt.context="/TecnicosSet" id="idtech"/>
					<ObjectAttribute title="{i18n>name}" text="{Tech>Name} {Tech&gt;Lastname}"/>
					<ObjectAttribute title="{i18n>tel}" text="{Tech>Tel}"/>
				</layout:VerticalLayout>

				<layout:VerticalLayout>
					<ObjectAttribute title="{i18n>province}" text="{Tech>Province}"/>

					<ObjectStatus text="{Tech>City}" state="{/objectDescription/status}" title="{i18n>city}"/>
				</layout:VerticalLayout>
			</layout:HorizontalLayout>
		</semantic:headerContent>

        <semantic:content>
        <sap.ui.layout.form:SimpleForm xmlns:sap.ui.layout.form="sap.ui.layout.form" xmlns:sap.ui.core="sap.ui.core" xmlns:cd="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" editable="true" layout="ResponsiveGridLayout" id="form3" cd:w5g.dt.context="/TecnicosSet">

            <sap.ui.layout.form:content>

                <sap.ui.core:Title text="Personal Data" id="title8" emphasized="true" icon="sap-icon://employee"/>

                <Label text="{i18n>name}/{i18n&gt;lastName}" id="label2"/>

                <Input width="100%" id="name" placeholder="Name" maxLength="15" autocomplete="false" valueStateText="{i18n>textValidate}" value="{Tech>Name}" editable="false"/>

                <Input width="100%" id="lastName" placeholder="Last Name" maxLength="15" valueStateText="{i18n>textValidate}" value="{Tech>Lastname}" editable="false"/>

                <Label text="{i18n>tel}" id="label3"/>

                <Input width="100%" id="tel" placeholder="1123906798" maxLength="10" type="Tel" valueStateText="{i18n>tel}" value="{Tech>Tel}" editable="false"/>

                <Label text="{i18n>province}/{i18n&gt;city}/{i18n>street}/{i18n&gt;zip}" width="100%" id="label4"/>

                <Input xmlns="sap.m" id="province" placeholder="Code Province ex. BA" maxLength="3" valueStateText="{i18n>textValidate}" value="{Tech>Province}" editable="false"/>

                <Input xmlns="sap.m" id="city" placeholder="City" maxLength="15" valueStateText="{i18n>textValidate}" autocomplete="false" value="{Tech>City}" editable="false"/>

                <Input xmlns="sap.m" id="street" placeholder="Street" maxLength="30" valueStateText="{i18n>textValidate}" value="{Tech>Street}" editable="false"/>

                <Input xmlns="sap.m" id="zip" placeholder="ZIP" maxLength="10" type="Number" valueStateText="{i18n>zipValidate}" editable="false" value="{Tech>Zip}"/></sap.ui.layout.form:content>

            </sap.ui.layout.form:SimpleForm>
        </semantic:content>

    <semantic:deleteAction>

        <semantic:DeleteAction id="delete"/>

    </semantic:deleteAction>

    <semantic:titleMainAction>

        <semantic:TitleMainAction text="Assign" id="assign"/>

    </semantic:titleMainAction>

    <semantic:fullScreenAction>

        <semantic:FullScreenAction tooltip="Full Screen" id="action2"/>

    </semantic:fullScreenAction>

    </semantic:SemanticPage>

</mvc:View>