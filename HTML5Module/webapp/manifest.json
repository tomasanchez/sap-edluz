{
  "_version": "1.12.0",
  "sap.app": {
    "id": "ns.HTML5Module",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "ach": "ach",
    "sourceTemplate": {
      "id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
      "version": "1.40.12"
    },
    "dataSources": {
      "mainService": {
        "uri": "sap/opu/odata/sap/ZOS_CA_LUUC3_EDELUZ_SRV/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "localUri": "localService/metadata.xml"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "rootView": {
      "viewName": "ns.HTML5Module.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    },
    "dependencies": {
      "minUI5Version": "1.60.1",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ui.layout": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "ns.HTML5Module.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "defaultBindingMode": "TwoWay",
          "defaultCountMode": "Inline",
          "refreshAfterChange": false
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "ns.HTML5Module.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "appHome",
          "pattern": "",
          "target": [
            "Home"
          ]
        },
        {
            "name": "techsList",
            "pattern": "techs",
			"target": "Techs"
        },        
        {
            "name": "tech",
            "pattern": "techs/byid",
			"target": "Tech"
        },
        {
            "name": "newTech",
            "pattern": "techs/new",
			"target": "NewTech"
        },
        {
            "name": "complaint",
            "pattern": "complaint",
            "target": "Reclamos"
        },
        {
            "name": "customer",
            "pattern": "customer",
            "target": "Customer"
        }
      ],
      "targets": {
        "Home": {
          "viewType": "XML",
          "viewName": "Home",
          "viewLevel": 0
        },
        "Techs":{
            "viewType": "XML",
            "viewName": "Techs",
            "viewLevel": 1
        },
        "Tech":{
            "viewType": "XML",
            "viewName": "Tech",
            "viewLevel": 3
        },
        "NewTech":{
            "viewType": "XML",
            "viewName": "NewTech",
            "viewLevel": 2
        },
        "Reclamos":{
            "viewType": "XML",
            "viewName": "Reclamos",
            "viewLevel": 1
        },
        "Customer":{
            "viewType": "XML",
            "viewName": "Customer",
            "viewLevel": 1
        }
      }
    }
  }
}